{
  "kind": "collectionType",
  "collectionName": "patients",
  "info": {
    "singularName": "patient",
    "pluralName": "patients",
    "displayName": "Patient"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "phone": {
      "type": "biginteger",
      "required": false,
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "closed",
        "new"
      ]
    },
    "assigned_dr": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "patients"
    },
    "admit_date": {
      "type": "date"
    },
    "age": {
      "type": "integer"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "other"
      ]
    },
    "answer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::answer.answer",
      "inversedBy": "patient"
    }
  }
}
